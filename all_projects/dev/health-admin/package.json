{
  "name": "pretaa-admin",
  "version": "1.0.0",
  "private": true,
  "engines" : { 
    "npm" : ">=8.19.4",
    "node" : ">=16.0.0 <18"
  },
  "scripts": {
    "fetch-schema": "npx apollo client:download-schema",
    "codegen": "npx apollo client:codegen --target typescript --outputFlat=src/health-generatedTypes.ts",
    "codegen:env": "dotenv -e .env npm run codegen",
    "fetch-schema:env": "dotenv -e .env npm run fetch-schema",
    "fetch-schema:test": "dotenv -e .env.test npm run fetch-schema",
    "fetch-schema:dev": "dotenv -e .env.dev npm run fetch-schema",
    "fetch-schema:client-dev": "dotenv -e .env.client-dev npm run fetch-schema",
    "fetch-schema:prod": "dotenv -e .env.prod npm run fetch-schema",
    "fetch-schema:local": "dotenv -e .env.local npm run fetch-schema",
    "fetch-schema:edge": "dotenv -e .env.edge npm run fetch-schema",
    "start": "craco start",
    "start:dev": "dotenv -e .env.dev npm run start",
    "start:local": "dotenv -e .env.local npm run start",
    "start:test": "dotenv -e .env.test npm run start",
    "start:prod": "dotenv -e .env.prod npm run start",
    "start:client-demo": "dotenv -e .env.client-demo npm run start",
    "start:client-dev": "dotenv -e .env.client-dev npm run start",
    "start:edge": "dotenv -e .env.edge npm run start",
    "codegen:local": "dotenv -e .env.local npm run codegen",
    "codegen:edge": "dotenv -e .env.edge npm run codegen",
    "codegen:dev": "dotenv -e .env.dev npm run codegen",
    "codegen:prod": "dotenv -e .env.prod npm run codegen",
    "codegen:client-dev": "dotenv -e .env.client-dev npm run codegen",
    "codegen:client-demo": "dotenv -e .env.client-demo npm run codegen",
    "codegen:client-prod": "dotenv -e .env.client-prod npm run codegen",
    "start:e2e": "dotenv -e .env.e2e npm run start",
    "codegen:e2e": "dotenv -e .env.e2e npm run codegen",
    "codegen:test": "dotenv -e .env.test npm run codegen",
    "fetch-schema:e2e": "dotenv -e .env.e2e npm run fetch-schema",
    "build": "craco build",
    "test": "craco test",
    "test:coverage:all": "craco test --coverage --watchAll",
    "test:ci": "craco test --watchAll=false",
    "test:coverage": "craco test --coverage",
    "eject": "craco eject",
    "format": "prettier --write ./**/**/**/**/**/**/*.ts{,x}",
    "eslint": "eslint",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src/**/*.ts{,x} --fix",
    "lint-staged": "lint-staged",
    "svg-icons": "svg-sprite --css --css-render-css --css-example --dest=out src/assets/icons/**/*.svg",
    "deploy:netlify": "netlify deploy",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "build:dev": "rm -rf build && dotenv -e .env.dev npm run build",
    "build:staging": "rm -rf build && dotenv -e .env.staging npm run build",
    "build:prod": "rm -rf build && dotenv -e .env.prod npm run build",
    "build:ci": "rm -rf build && dotenv -e .env.ci npm run build",
    "snyk": "snyk",
    "release": "release-it && npm run changelog",
    "pull": "git pull && git submodule update",
    "git:submodule:init": "git submodule update --init --remote --recursive",
    "reset:submodule:graphql": "git reset -- src/graphql"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": "eslint --max-warnings=0"
  },
